{
  "name": "react-shinto",
  "version": "0.1.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/designst/react-shinto"
  },
  "engines": {
    "npm": ">=3.0",
    "node": ">=6.0"
  },
  "author": "Manfred Haltner <info@designstage.ch>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env DEBUG=shinto:*,-shinto-verbose:* NODE_ENV=development NODE_PATH=./app nodemon ./index.js",
    "start:api": "cross-env DEBUG=shinto:* NODE_ENV=development NODE_PATH=./api node ./api/index.js",
    "start:prod": "yarn build && yarn start:production",
    "start:production": "cross-env NODE_ENV=production NODE_PATH=./app node ./index.js",
    "build": "yarn clean:build && cross-env NODE_ENV=production webpack --progress --hide-modules --config ./config/webpack/webpack.prod.babel.js",
    "build:dll": "node ./scripts/dependencies.js",
    "analyze": "yarn clean:build && cross-env NODE_ENV=analyze webpack --progress --hide-modules --config ./config/webpack/webpack.dev.babel.js",
    "docs": "esdoc && open ./docs/index.html",
    "extract-intl": "babel-node --presets @babel/preset-env,@babel/preset-stage-0 -- ./scripts/extract-intl.js",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "yarn lint:js && yarn lint:style",
    "lint:js": "eslint --fix ./app ./config ./index.js",
    "lint:style": "stylelint --fix \"./app/**/*.css\" \"./app/**/*.scss\"",
    "flow": "flow",
    "flow:stop": "flow stop",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "yarn test --watch",
    "clean:all": "yarn clean:build && yarn clean:test",
    "clean:build": "rimraf ./build",
    "clean:test": "rimraf ./coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls && yarn clean:test",
    "postsetup": "yarn build:dll",
    "postinstall": "yarn build:dll"
  },
  "babel": {
    "env": {
      "production": {
        "plugins": [
          "transform-remove-console",
          "transform-react-remove-prop-types",
          "@babel/transform-react-inline-elements",
          "@babel/transform-react-constant-elements"
        ]
      },
      "test": {
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    },
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": false,
          "useBuiltIns": "usage"
        }
      ],
      "@babel/preset-flow",
      "@babel/preset-react",
      "@babel/preset-es2015",
      [
        "@babel/preset-stage-0",
        {
          "decoratorsLegacy": true
        }
      ]
    ],
    "plugins": [
      "lodash",
      "dynamic-import-node",
      "react-hot-loader/babel",
      "loadable-components/babel"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "prettier",
      "prettier/react",
      "prettier/flowtype",
      "plugin:lodash-fp/recommended"
    ],
    "plugins": [
      "react",
      "prettier",
      "jsx-a11y",
      "lodash-fp",
      "redux-saga"
    ],
    "env": {
      "es6": true,
      "node": true,
      "jest": true,
      "browser": true
    },
    "rules": {
      "no-console": "off",
      "arrow-parens": "off",
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "global-require": "off",
      "no-param-reassign": "off",
      "object-curly-newline": "off",
      "no-underscore-dangle": "off",
      "function-paren-newline": "off",
      "import/no-dynamic-require": "off",
      "import/prefer-default-export": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ],
      "jsx-a11y/anchor-is-valid": [
        "error",
        {
          "components": [
            "Link"
          ],
          "specialLink": [
            "to"
          ]
        }
      ],
      "prettier/prettier": [
        "error",
        {
          "parser": "flow",
          "printWidth": 99,
          "singleQuote": true,
          "trailingComma": "all"
        }
      ],
      "lodash-fp/use-fp": "off",
      "lodash-fp/consistent-name": "off",
      "react/forbid-prop-types": "off",
      "react/prefer-stateless-function": "off",
      "react/jsx-filename-extension": [
        "error",
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./config/webpack/webpack.dev.babel.js"
        }
      }
    },
    "globals": {
      "__DEV__": true,
      "__CLIENT__": true,
      "__SERVER__": true
    }
  },
  "eslintIgnore": [
    "config/flow"
  ],
  "stylelint": {
    "extends": [
      "stylelint-config-standard"
    ]
  },
  "dllPlugin": {
    "name": "react-shinto-dll",
    "exclude": [
      "chalk",
      "compression",
      "cookie-parser",
      "cross-env",
      "dotenv",
      "express",
      "http",
      "http-proxy-middleware",
      "ip",
      "mockery",
      "morgan",
      "serve-favicon"
    ],
    "include": [
      "core-js",
      "lodash"
    ]
  },
  "jest": {
    "verbose": true,
    "globals": {
      "__DEV__": false,
      "__CLIENT__": true,
      "__SERVER__": true
    },
    "testMatch": [
      "<rootDir>/app/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/app/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/config/jest/setup.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/setup.test.js",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/transformers/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/transformers/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^utils(.*)$": "<rootDir>/app/utils/$1",
      "^config(.*)$": "<rootDir>/app/config/$1",
      "^providers(.*)$": "<rootDir>/app/providers/$1",
      "^components(.*)$": "<rootDir>/app/components/$1",
      "^containers(.*)$": "<rootDir>/app/containers/$1"
    },
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,mjs}",
      "!app/index.js",
      "!app/theme.js",
      "!app/routes/**",
      "!app/utils/registerServiceWorker.js"
    ]
  },
  "nodemonConfig": {
    "watch": [
      "app/routes/**",
      "server/",
      "server/middlewares/"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.46",
    "@babel/register": "7.0.0-beta.46",
    "@rematch/core": "1.0.0-alpha.2",
    "@rematch/loading": "1.0.1",
    "axios": "0.18.0",
    "chalk": "2.4.1",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cross-env": "5.1.4",
    "debug": "3.1.0",
    "dotenv": "5.0.1",
    "dotenv-expand": "4.2.0",
    "express": "4.16.3",
    "helmet": "3.12.0",
    "history": "4.7.2",
    "hoist-non-react-statics": "2.5.0",
    "hpp": "0.2.2",
    "http-proxy-middleware": "0.18.0",
    "humps": "2.0.1",
    "intl": "1.2.5",
    "invariant": "2.2.4",
    "ip": "1.1.5",
    "jss": "9.8.1",
    "loadable-components": "1.4.0",
    "lodash": "4.17.10",
    "material-ui": "1.0.0-beta.43",
    "morgan": "1.9.0",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-helmet": "5.2.0",
    "react-hot-loader": "4.1.3",
    "react-intl": "2.4.0",
    "react-jss": "8.4.0",
    "react-redux": "5.0.7",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.9",
    "redux": "4.0.0",
    "redux-auth-wrapper": "2.0.2",
    "redux-form": "7.3.0",
    "redux-form-material-ui": "5.0.0-beta.2",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "redux-wait-for-action": "0.0.7",
    "reselect": "3.0.1",
    "serialize-javascript": "1.5.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.46",
    "@babel/core": "7.0.0-beta.46",
    "@babel/node": "7.0.0-beta.46",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.46",
    "@babel/plugin-transform-react-constant-elements": "7.0.0-beta.46",
    "@babel/plugin-transform-react-inline-elements": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-es2015": "7.0.0-beta.46",
    "@babel/preset-flow": "7.0.0-beta.46",
    "@babel/preset-react": "7.0.0-beta.46",
    "@babel/preset-stage-0": "7.0.0-beta.46",
    "asset-require-hook": "1.2.0",
    "autoprefixer": "8.4.1",
    "axios-mock-adapter": "1.15.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "8.2.3",
    "babel-jest": "23.0.0-beta.2",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-dynamic-import-node": "1.2.0",
    "babel-plugin-lodash": "3.3.2",
    "babel-plugin-react-intl": "2.4.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.13",
    "babel-plugin-transform-remove-console": "6.9.2",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "coveralls": "3.0.1",
    "css-loader": "0.28.11",
    "css-modules-require-hook": "4.2.3",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.9.0",
    "eslint-loader": "2.0.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-lodash-fp": "2.1.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-redux-saga": "0.8.0",
    "file-loader": "1.1.11",
    "flow-bin": "0.71.0",
    "glob": "7.1.2",
    "html-minifier": "3.5.15",
    "imagemin-webpack-plugin": "2.1.1",
    "jest": "23.0.0-beta.2",
    "mini-css-extract-plugin": "0.4.0",
    "mkdirp": "0.5.1",
    "ngrok": "3.0.1",
    "node-sass": "4.9.0",
    "nodemon": "1.17.4",
    "optimize-css-assets-webpack-plugin": "4.0.1",
    "plop": "2.0.0",
    "postcss-flexbugs-fixes": "3.3.1",
    "postcss-loader": "2.1.5",
    "prettier": "1.12.1",
    "raf": "3.4.0",
    "react-addons-test-utils": "15.6.2",
    "react-dev-utils": "5.0.1",
    "react-test-renderer": "16.3.2",
    "rimraf": "2.6.2",
    "sass-loader": "7.0.1",
    "shelljs": "0.8.2",
    "style-loader": "0.21.0",
    "stylelint": "9.2.0",
    "stylelint-config-standard": "18.2.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglifyjs-webpack-plugin": "1.2.5",
    "url-loader": "1.0.1",
    "webpack": "4.8.1",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.1.3",
    "webpack-dev-middleware": "3.1.3",
    "webpack-hot-middleware": "2.22.1",
    "webpack-manifest-plugin": "2.0.2"
  }
}
